<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="gunwerks_blog_post_cover" model="ir.attachment">
            <field name="public" eval="True"/>
            <field name="name">Blog Post Default Cover Image</field>
            <field name="datas_fname">Blog Post Default Cover Image</field>
            <field name="res_model">ir.ui.view</field>
            <field name="type">url</field>
            <field name="url">/theme_gunwerks/static/src/img/blog_post_cover_1.jpg</field>
        </record>

        <record id="gunwerks_category_cover" model="ir.attachment">
            <field name="public" eval="True"/>
            <field name="name">Category Header Image</field>
            <field name="datas_fname">Category Header Image</field>
            <field name="res_model">ir.ui.view</field>
            <field name="type">url</field>
            <field name="url">/theme_gunwerks/static/src/img/category_header_image.jpg</field>
        </record>

        <record id="gunwerks_blog_post_type_1" model="blog.post.type">
            <field name="active" eval="True"/>
            <field name="name">Long Range Pursuit Blog</field>
        </record>

        <record id="gunwerks_blog_post_type_2" model="blog.post.type">
            <field name="active" eval="True"/>
            <field name="name">Long Range Pursuit Podcast</field>
        </record>

        <record id="gunwerks_blog_post_type_3" model="blog.post.type">
            <field name="active" eval="True"/>
            <field name="name">Long Range Pursuit TV</field>
        </record>

        <record id="gunwerks_blog_post_type_4" model="blog.post.type">
            <field name="active" eval="True"/>
            <field name="name">Press Release</field>
        </record>

    </data>
</odoo>

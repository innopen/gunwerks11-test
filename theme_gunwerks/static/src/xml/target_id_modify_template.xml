<?xml version="1.0" encoding="UTF-8"?>
<templates xml-space="preserve">
  <t t-name="theme_gunwerks.target_id_section_modify">
    <div class="modal fade" contentEditable="False" role="dialog">
      <div class="modal-dialog">
        <!-- Modal Content -->
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">&amp;times</button>
            <h4>Target ID Section</h4>
          </div>
          <div class="modal-body">
            <div class="modify_id_container container">
              <div class="row">
                <label for="modify_id_value" class="col-sm-3 control-label">ID Value</label>
                <div class="col-sm-9">
                  <input name="modify_id_value" type="text" placeholder="Enter Some Text" value=""/>
                </div>
              </div>
            </div>
          </div>
          <div class="modal-footer">
            <button id="btn_cancel" data-dismiss="modal" class="btn btn-primary">Cancel</button>
            <button id="btn_apply" data-dismiss="modal" class="btn btn-success">Apply</button>
          </div>
        </div>
      </div>
    </div>
  </t>
</templates>
